{
    "comment": "Sample component configuration",
    "components": {
        "Welcome Component": {
            "window": {
                "url": "$applicationRoot/components/welcome/welcome.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 400,
                "height": 370
            },
            "component": {
                "inject": false,
                "spawnOnStartup": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "welcome"
                    }
                }
            }
        },
        "Notepad": {
            "window": {
                "id": "Notepad",
                "native": true,
                "path": "notepad.exe",
                "url": "",
                "defaultHeight": 600,
                "autoShow": true,
                "alwaysOnTop": false,
                "resizable": true,
                "showTaskbarIcon": false,
                "contextMenu": true,
                "addToWorkspace": true
            },
            "component": {
                "spawnOnHotkey": [
                    "ctrl",
                    "alt",
                    "n"
                ]
            },
            "foreign": {
                "services": {
                    "workspaceService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": true
                    },
                    "Toolbar": {
                        "iconURL": "$applicationRoot/components/assets/img/notepad.png"
                    }
                }
            }
        },
        "Getting Started Tutorial": {
            "window": {
                "url": "https://documentation.chartiq.com/finsemble/tutorial-gettingStarted.html",
