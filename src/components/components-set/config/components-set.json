{
  "comment": "fpe ascent configuration",
  "components": {
    "Ascent": {
      "auth": {
        "formIndex": 0,
        "userInput": "user[email]",
        "passInput": "user[password]"
      },
      "component": {
        "inject": "$applicationRoot/components/components-set/ascent.js"
      },
      "window": {
        "url": "https://app.ascentregtech.com/users/sign_in",
        "resizable": true,
        "addToWorkspace": true,
        "top": "center",
        "left": "center",
        "width": 800,
        "height": 600
      },
      "foreign": {
        "services": {
          "dockingService": {
            "canGroup": true,
            "isArrangable": true
          }
        },
        "components": {
          "App Launcher": {
            "launchableByUser": true
          },
          "Window Manager": {
            "FSBLHeader": true,
            "persistWindowState": true,
            "showLinker": true
          },
          "Toolbar": {
            "iconClass": "fpe-ascent"
          }
        }
      }
    },
    "Corlytics": {
