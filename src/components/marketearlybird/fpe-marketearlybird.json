{
    "comment": "Sample component configuration",
    "components": {
        "EarlyBird": {
            "window": {
                "url": "https://beta-app.marketearlybird.com",
                "icon": "https://beta-app.marketearlybird.com/img/Earlybird_icon_up.png",
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 500,
                "height": 800
            },
            "component": {
                "inject": false,
                "spawnOnStartup": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "showLinker": true,
                        "alwaysOnTopIcon": true,
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconURL": "https://beta-app.marketearlybird.com/img/Earlybird_icon_up.png"
                    }
                }
            }
        },
        "EarlyBird-prod": {
            "window": {
                "url": "https://app.marketearlybird.com",
                "icon": "https://app.marketearlybird.com/img/Earlybird_icon_up.png",
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 500,
                "height": 800
            },
            "component": {
                "inject": false,
                "spawnOnStartup": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "showLinker": true,
                        "alwaysOnTopIcon": true,
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconURL": "https://beta-app.marketearlybird.com/img/Earlybird_icon_up.png"
                    }
                }
            }
        },
        "Linker Tester": {
            "window": {
                "url": "$applicationRoot/components/linkerTester/linkerTester.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 330,
                "height": 520
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    },
                    "workspace": {
                        "persistURL": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "title": "Linker Tester",
                        "showLinker": true,
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "linkerTester"
                    }
                }
            }
        }
    }
}
