{
	"components": {
		"Advanced Chart": {
			"window": {
				"url": "https://demo.chartiq.com",
				"width": 800,
				"height": 600,
				"options": {
					"minWidth": 75
				}
			},
			"component": {
				"mode": "premium",
				"advertiseReceivers": [
					"chartiq.chart",
					"symbol"
				],
				"spawnOnHotkey": [
					"ctrl",
					"alt",
					"a"
				],
				"category": "system",
				"preload": "$applicationRoot/components/chartiq/chartiq.js"
			},
			"foreign": {
				"services": {
					"dockingService": {
						"isArrangable": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Toolbar": {
						"iconClass": "ff-chart-advanced"
					},
					"Window Manager": {
						"title": "ChartIQ",
						"FSBLHeader": true,
						"showLinker": true,
						"persistWindowState": true
					}
				}
			}
		}
	}
}