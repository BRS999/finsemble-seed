{
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "off",
            "tab"
        ],
        "linebreak-style": [
            "off",
            "unix"
        ],
        "quotes": [
            "off",
            "double"
        ],
        "semi": [
            "off",
            "always"
        ],
        "no-unused-vars": "off",
        "no-case-declarations": "off",
        "no-extra-semi": "off",
        "no-inner-declarations": "off",
        "no-console": "off"
    },
    "globals": {
        "FSBL": true,
        "fin": true
    },
    "settings": {
        "react": {
            "version": "15.0"
        }
    }
}